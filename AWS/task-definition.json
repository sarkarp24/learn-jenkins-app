{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearnJenkinsApp-TaskDefinition-Prod",
    "containerDefinitions": [
        {
            "name": "LearnJenkinsApp",
            "image": "prodyotsarkar/insure-me:1.0",
            "portMappings": [{
                "name": "learn-jenkinsapp-80-tcp",
                "containerPort": 80,
                "hostPort": 80,
                "protocol": "tcp",
                "appProtocol": "http"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::762233725823:role/ecsTaskExecutionRole"
}
